<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".wode.ServiceActivity">
    <include android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/templatenavigationbarreturn2"></include>
    <com.mdb.example.administrator.view.XListView
        android:id="@+id/service_xlistview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.mdb.example.administrator.view.XListView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:orientation="vertical"
        android:gravity="center">
        <Button
            android:id="@+id/fuwu_zanting"
            android:layout_marginTop="20dp"
            android:layout_width="200dp"
            android:layout_height="35dp"
            android:text="暂停网上预约"
            android:textSize="15sp"
            android:textColor="@color/white"
            android:background="@mipmap/buttons"/>
    </LinearLayout>
</LinearLayout>