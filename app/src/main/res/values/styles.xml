<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="textview_style">
        <item name="android:layout_height">@dimen/y105</item>
        <item name="android:layout_width">@dimen/y105</item>
        <item name="android:layout_marginRight">@dimen/y12</item>
        <item name="android:background">@mipmap/baikuang</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/xuanzhong</item>
        <item name="android:textSize">24dp</item>
    </style>
</resources>
